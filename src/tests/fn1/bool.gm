fn return_neg(b: bool) => bool {
    return !b;
}

fn return_bool() => bool {
    return true;
}

fn bool_loop() => u32 {
    var stop: bool = false;
    var k: i32 = -1;
    for {
        if stop {
            return 27;
        }
        if k >= 2034 {
            stop = true;
        }
        k += 1;
    }
}
