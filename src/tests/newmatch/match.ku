fun use_if_match(x: uint) => uint {
    if x
    => 1, 2, 4 { return 2; }
    => 3 {
        let a := 1;
        return a;
    }
    else { return x; }
}

type Color enum u8 {
    RED,
    GREEN,
    BLUE,
}

fun colot_to_int(c: Color) => u8 {
    if c
    => .RED { return 1; }
    => .GREEN { return 2; }
    => .BLUE { return 3; }

    // never marks unreachable code path
    // never;
}

