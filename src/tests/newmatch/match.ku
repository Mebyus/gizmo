fun use_if_match(x: uint) => uint {
    if x
    case 1, 2, 4 { return 2; }
    case 3 {
        a := 1;
        return a;
    }
    else { return x; }
}

fun use_if_bool_match(x: uint) => uint {
    if
    case x == 1 {}
    case x > 2 { return 2; }
    case 2 * x < 10 { 
        let a := 2;
        return a + x;
    }
    else { return 1; }

    return 3;
}
