type Result enum u32 {
    Ok      = 0,
    Error   = 1,
    Unknown = 2,
}

fn [Result] is_ok() => bool {
    return rv == Result.Ok;
}

fn [Result] is_err() => bool {
    return rv == Result.Error;
}

fn add(a: u32, b: u32) => u32 {
    let c: u32 = a + b;
    return c;
}

const max_u32: u32 = 0xffffffff;

