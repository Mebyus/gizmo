fn inc(x: *int) => int {
    x.@ += 1;
    let x2: int = x.@;

    if x2 >= 19 {
        return 19;
    }
    return x2;
}

fn use_inc() {
    var x: int = 1;
    let r: int = inc(x.&);
}

fn test_dead_code(a: int) {
    var x: int = a;
    var x: str = "";
    return;

    let r: int = inc(x.&);
}
