fn another_add_function(a: f32, b: f32) => f32 {
    return a + b;
}

fn compare_empty_string(s: str) => bool {
    if s == "" {
        return true;
    }
    return s == "<nil>";
}

fn for_each_loop_example(k: int) => int {
    var s: int = 0;
    for let i in range(k) {
        s += i;
    }
    return s;
}
