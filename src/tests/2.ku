fun another_add_function(a: f32, b: f32) => f32 {
    return a + b;
}

fun compare_empty_string(s: str) => bool {
    if s == "" {
        return true;
    }
    return s == "<nil>";
}

fun for_each_loop_example(k: sint) => sint {
    var s: sint = 0;
    for i in range(k) {
        s += i;
    }
    return s;
}
